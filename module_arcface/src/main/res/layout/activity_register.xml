<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="ResourceName">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".arcface.RegisterActivity">

        <Button
            android:id="@+id/importface"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="导入人脸"></Button>

        <Button
            android:id="@+id/claerface"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="清除人脸"></Button>

        <Button
            android:id="@+id/recface"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="开始识别"></Button>


        <Button
            android:id="@+id/download"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="下载图片"></Button>


        <Button
            android:id="@+id/download_rec"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="下载图片并识别"></Button>
    </LinearLayout>
</layout>